QDialog {background-color: #f5f5f5}
QLabel {text-align: center}

QPushButton {border:none;background-repeat:no-repeat;outline-style: none;}

/* main */
    /* Startup */
    QPushButton#Home_Startup_btOpenDoor {
    font-size:45px;
    background: url(:/images/opendoor.png) no-repeat transparent top center;
    padding-top: 300;
    }
    /* Login */
    QPushButton#Home_Login_btLogin {
    font-size: 40px;
    color: white;
    padding-left: 50px;
    padding-bottom: 10px;
    background: url(:/images/loginbt.png) no-repeat transparent;
    }
    /* Idle */
    QPushButton#Home_Idle_btOpenDoor {
    background: url(:/images/opendoor.png) no-repeat transparent;
    }
    QPushButton#Home_Idle_btQuit {
    font-size: 40px;
    padding-left: 30px;
    padding-bottom: 4px;
    background: url(:/images/btclose.png) no-repeat transparent;
    }
    QPushButton#Home_Idle_btLogout {
    font-size: 40px;
    padding-left: 50px;
    padding-bottom: 4px;
    background: url(:/images/loginquit.png) no-repeat transparent;
    }
    QLabel#Home_Idle_lbBoxState {
    font-size: 30px;
    }
